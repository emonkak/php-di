{
    "name": "emonkak/di",
    "description": "A fast dependency injection library for PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Nozaki",
            "email": "emonkak@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "jeremeamia/SuperClosure": ">=1.0",
        "container-interop/container-interop": "~1.1"
    },
    "require-dev": {
        "doctrine/annotations": "~1.2",
        "phpunit/phpunit": "~4.4",
        "satooshi/php-coveralls": "~1.0",
        "symfony/process": "~2.6"
    },
    "conflict": {
        "phpdocumentor/reflection-docblock": ">=3.0"
    },
    "suggest": {
        "doctrine/annotations": "Used for AnnotationInjectionPolicy"
    },
    "autoload": {
        "psr-4": {
            "Emonkak\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emonkak\\Di\\Tests\\": "tests/"
        }
    }
}
