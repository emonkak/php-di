{
    "name": "emonkak/di",
    "description": "A fast dependency injection library for PHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Nozaki",
            "email": "emonkak@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "jeremeamia/superclosure": "^2.0",
        "container-interop/container-interop": "^1.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.2",
        "phpunit/phpunit": "^5.7",
        "satooshi/php-coveralls": "^2.0",
        "symfony/filesystem": "^3.0",
        "symfony/process": "^3.0"
    },
    "suggest": {
        "doctrine/annotations": "Used for AnnotationInjectionPolicy",
        "symfony/filesystem": "Used for ServiceProviderLoaderOnFilesystem"
    },
    "autoload": {
        "psr-4": {
            "Emonkak\\Di\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Emonkak\\Di\\Tests\\": "tests/"
        }
    }
}
